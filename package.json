{
  "name": "bioinfocore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true --output-style compressed src/scss/ -o public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/**/*.scss' -c 'npm run sass:build'",
    "dev-scss": "npm-run-all -p sass:*",
    "dev-js": "nodemon src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MusicDev33/bioinfocore.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/MusicDev33/bioinfocore#readme",
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "chokidar-cli": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "node-sass": "^5.0.0",
    "node-sass-chokidar": "^1.5.0",
    "node-sass-middleware": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.2",
    "nunjucks-setasync": "^1.0.4",
    "path": "^0.12.7",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "sass": "^1.30.0"
  }
